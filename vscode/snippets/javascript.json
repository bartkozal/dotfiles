{
  "log": {
    "prefix": "log",
    "body": ["console.log($0) "],
    "description": "Logs output to console"
  },
  "return function": {
    "prefix": "fun",
    "body": ["$1 => $0"],
    "description": "Inserts return function"
  },
  "arrow function": {
    "prefix": "func",
    "body": ["($1) => {", "  $0", "}"],
    "description": "Inserts arrow function"
  },
  "component": {
    "prefix": "comp",
    "body": [
      "import React, { ${2|Component,PureComponent|} } from 'react';",
      "",
      "class $1 extends ${2|Component,PureComponent|} {",
      "  $0",
      "}",
      "",
      "export default $1;"
    ],
    "description": "Inserts component"
  },
  "stateless component": {
    "prefix": "com",
    "body": [
      "import React from 'react';",
      "",
      "const $1 = (${3:{ $2 \\}}) => (",
      "  $0",
      ");",
      "",
      "export default $1;"
    ],
    "description": "Inserts stateless component"
  },
  "jest description": {
    "prefix": "des",
    "body": ["describe('$1', () => {", "  $0", "});"],
    "description": "Inserts test description"
  },
  "jest it": {
    "prefix": "it",
    "body": ["it('$1', () => {", "  $0", "});"],
    "description": "Inserts test description"
  },
  "jest test": {
    "prefix": "test",
    "body": ["test('$1', () => {", "  $0", "});"],
    "description": "Inserts test description"
  },
  "import": {
    "prefix": "imp",
    "body": [
        "import $1 from '$2';$0"
    ],
    "description": "Inserts import statement"
  }
}
