{
  "log": {
    "prefix": "log",
    "body": ["console.log($1) "],
    "description": "Logs output to console"
  },
  "return function": {
    "prefix": "fun",
    "body": ["${1:($2)} => ${3:($4)}"],
    "description": "Inserts return function"
  },
  "arrow function": {
    "prefix": "func",
    "body": ["($1) => {", "  $2", "}"],
    "description": "Inserts arrow function"
  },
  "component": {
    "prefix": "comp",
    "body": [
      "import React, { Component } from 'react';",
      "",
      "class $1 extends Component {",
      "  $2",
      "}",
      "",
      "export default $1;"
    ],
    "description": "Inserts component"
  },
  "stateless component": {
    "prefix": "com",
    "body": [
      "import React from 'react';",
      "",
      "const $1 = ({ $2 }) => (",
      "  $3",
      ");",
      "",
      "export default $1;"
    ],
    "description": "Inserts stateless component"
  },
  "test description": {
    "prefix": "des",
    "body": ["describe('$1', () => {", "  $2", "});"],
    "description": "Inserts test description"
  },
  "test it": {
    "prefix": "it",
    "body": ["it('$1', () => {", "  $2", "});"],
    "description": "Inserts test description"
  }
}
