{
  "bracketPairColorizer.activeScopeCSS": [
    "backgroundColor : {color}",
    "borderStyle : solid",
    "borderWidth : 1px",
    "borderColor : {color}; opacity: 0.5"
  ],
  "bracketPairColorizer.highlightActiveScope": true,
  "breadcrumbs.enabled": true,
  "editor.autoIndent": true,
  "editor.fontFamily": "IBMPlexMono",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.gotoLocation.multiple": "goto",
  "editor.highlightActiveIndentGuide": false,
  "editor.matchBrackets": false,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": true,
  "editor.renderIndentGuides": true,
  "editor.rulers": [
    80
  ],
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "onlySnippets",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/.cache": true,
    "**/node_modules": true,
    "**/flow-typed": true,
    "**/tmp": true,
    "**/log": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "javascript.validate.enable": false,
  "jest.enableCodeLens": false,
  "search.smartCase": true,
  "search.location": "panel",
  "search.useIgnoreFiles": true,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.fontFamily": "Roboto Mono",
  "terminal.integrated.fontSize": 14,
  "window.openFoldersInNewWindow": "on",
  "window.restoreWindows": "all",
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "lucy",
  "workbench.editor.tabSizing": "shrink",
  "workbench.colorCustomizations": {
    "editor.findMatchBorder": "#9735c5",
    "editor.findMatchHighlightBackground": "#4e31539a",
    "editor.findMatchHighlightBorder": "#f700ff9a",
    "editor.selectionBackground": "#2a5679",
    "editor.selectionHighlightBackground": "#2a567940",
    "editor.selectionHighlightBorder": "#689eca",
    "editor.wordHighlightBackground": "#00ffff40",
    "editor.wordHighlightBorder": "#5aca76",
    "editor.wordHighlightStrongBackground": "#00ffff40",
    "editor.wordHighlightStrongBorder": "#5aca76"
  },
  "workbench.fontAliasing": "default",
  "workbench.iconTheme": "vs-seti",
  "workbench.list.horizontalScrolling": true,
  "workbench.list.keyboardNavigation": "filter",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.renderIndentGuides": "none",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue"
}
